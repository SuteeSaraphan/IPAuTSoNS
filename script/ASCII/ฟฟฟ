
apiVersion: extensions/v1beta1

kind: Ingress

metadata:
  name: ipautsons-ingress
spec:
  rules:
    - host: kubernetes.docker.internal
      http:
       paths:
        - path: /
          backend:
            serviceName: frontend-service
            servicePort: 80
        
        - path: /
          backend:
            serviceName: backend-service
            servicePort: 8000