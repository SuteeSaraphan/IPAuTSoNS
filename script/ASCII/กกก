apiVersion: networking.k8s.io/v1
kind: Ingress
metadata:
  name: http-ingress-backend
spec:
  rules:
  - host: 161.246.5.53
    http:
      paths:
      - path: /
        pathType: Prefix
        backend:
          service:
            name: backend-service
            port:
              number: 8000